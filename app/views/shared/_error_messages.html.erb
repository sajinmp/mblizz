<div id = "error_explain">
  <% if object.errors.any? %>
    <div class = "alert alert-danger">This form has  <%= pluralize(object.errors.count, "error") %></div>
    <ul>
      <% object.errors.full_messages.each do |err| %>
        <li> <%= err %> </li>
      <% end %>
    </ul>
  <% end %>
</div>
